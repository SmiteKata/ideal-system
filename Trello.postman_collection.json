						"exec": [
							"// Test oczekuje statusu 404. Jeśli Trello zwróci 404, ten test ZALICZY.\r",
							"pm.test(\"Status 404 - Resource not found\", function() {\r",
							"    pm.response.to.have.status(404);\r",
							"});\r",
							"\r",
							"// WERYFIKACJA KONTEKSTU: Czy na pewno pytaliśmy o nasze usunięte ID?\r",
							"pm.test(\"Weryfikacja URL - upewnienie się, że szukaliśmy właściwego ID\", function() {\r",
							"    var deletedID = pm.collectionVariables.get(\"boardID\");\r",
							"    \r",
							"    // Sprawdzamy, czy URL, na który wysłaliśmy żądanie, zawiera nasze ID\r",
							"    pm.expect(pm.request.url.toString()).to.include(deletedID);\r",
							"});\r",
							"\r",
							"// Dodatek: Czy treść błędu to typowy komunikat Trello?\r",
							"pm.test(\"Komunikat błędu jest poprawny\", function() {\r",
							"    // Sprawdzamy tekst odpowiedzi, a nie JSON\r",
							"    pm.expect(pm.response.text()).to.include(\"not found\");\r",
							"});"
